<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sares.View.Mesero2Controller">
   <children>
      <HBox fx:id="hbox" prefHeight="42.0" prefWidth="640.0">
         <children>
            <Label fx:id="nombre" text="Nombre" />
            <Label fx:id="tiempo" text="Label" />
            <Button mnemonicParsing="false" onMouseClicked="#handleSignOutActionhandleSignOutAction" text="Sign Out" />
         </children>
      </HBox>
      <Pane fx:id="pane_medio" prefHeight="94.0" prefWidth="640.0">
         <children>
            <Label fx:id="cuenta" layoutX="48.0" layoutY="32.0" text="# de Cuentas" />
            <TextField fx:id="cuentaText" layoutX="144.0" layoutY="27.0" />
            <Label fx:id="mesa" layoutX="55.0" layoutY="62.0" text="# de Mesa" />
            <TextField fx:id="mesaText" layoutX="144.0" layoutY="57.0" />
         </children>
      </Pane>
      <ListView fx:id="menu_Options" onMouseClicked="#seleccionMenu" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="buttonGoBack" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#handleGoBack" text="Finalizar Pedido" textAlignment="CENTER" />
   </children>
</VBox>
