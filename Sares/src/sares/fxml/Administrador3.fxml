<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sares.Controller.Mesero2Controller">
   <children>
      <HBox fx:id="hbox" prefHeight="26.0" prefWidth="640.0">
         <children>
            <Label fx:id="nombre" text="Nombre" />
            <Label fx:id="tiempo" text="Label" />
            <Button mnemonicParsing="false" onMouseClicked="#handleSignOutAction" text="Sign Out" />
         </children>
      </HBox>
      <Pane fx:id="pane_medio" prefHeight="60.0" prefWidth="640.0">
         <children>
            <Label fx:id="mesa" layoutX="130.0" layoutY="22.0" text="Nombre" />
            <TextField fx:id="mesaText" layoutX="220.0" layoutY="18.0" />
         </children>
      </Pane>
      <ListView fx:id="menu_Options" onMouseClicked="#seleccionMenu" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="buttonGoBack" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#handleGoBack" text="Guardar" textAlignment="CENTER" />
   </children>
</VBox>
